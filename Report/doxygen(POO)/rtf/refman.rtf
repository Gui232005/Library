896         {\cf19 if} (mes > 12)\par
897         \{ {\cf20 // Se o m\'EAs ultrapassar 12, avan\'E7a para o pr\'F3ximo ano}\par
898             mes = 1;\par
899             ano++;\par
900         \}\par
901     \}\par
902 \par
903     {\cf18 string} mesFinal, diaFinal;\par
904 \par
905     {\cf19 if} (dia < 10)\par
906         diaFinal = {\cf22 "0"} + to_string(dia);\par
907     {\cf19 if} (mes < 10)\par
908         mesFinal = {\cf22 "0"} + to_string(mes);\par
909 \par
910     DataEntrega = diaFinal + {\cf22 "-"} + mesFinal + {\cf22 "-"} + to_string(ano);\par
