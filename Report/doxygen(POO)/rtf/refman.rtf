\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 847 \{\par
848     Uteis *U = {\cf17 nullptr};\par
849 \par
850     Emprestimos *E = atribuirEmprestimo(idP);\par
851 \par
852     {\cf18 string} DataEntrega;\par
853     {\cf18 string} diaA, mesA, anoA;\par
854 \par
855     {\cf18 int} contador = 0;\par
856 \par
857     {\cf18 string} dataAtual = U->dataAtual();\par
858 \par
859     regex del({\cf22 "-"});\par
860 \par
861     sregex_token_iterator i(dataAtual.begin(), dataAtual.end(), del, -1);\par
862 \par
863     sregex_token_iterator end;\par
864 \par
865     {\cf19 while} (i != end)\par
866     \{\par
867         {\cf19 if} (contador == 0)\par
868         \{\par
869             diaA = *i;\par
870         \}\par
871         {\cf19 else} {\cf19 if} (contador == 1)\par
872         \{\par
873             mesA = *i;\par
874         \}\par
875         {\cf19 else} {\cf19 if} (contador == 2)\par
876         \{\par
